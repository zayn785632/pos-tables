  CREATE TABLE "CUST_INFO" 
   (	"ID" NUMBER, 
	"CUST_NAME" VARCHAR2(100), 
	"CUST_ADDRESS" VARCHAR2(100), 
	"ADDRESS2" VARCHAR2(50), 
	"PHONE_NO" NUMBER, 
	"NATIONALITY" NUMBER, 
	"GENDER" NUMBER, 
	"RELIGION" NUMBER, 
	"CREDIT_LIMT" NUMBER, 
	"CREATE_BY" VARCHAR2(40), 
	"CREATE_ON" DATE, 
	"COMPANY_ID" NUMBER, 
	"ACTIVE" NUMBER, 
	 PRIMARY KEY ("ID")
  USING INDEX  ENABLE
   ) ;
